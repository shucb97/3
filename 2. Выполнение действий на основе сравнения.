
#include <stdio.h>            
int main()
{
  float x,y,z;
 printf("Введите первое число");
scanf("%f", &x);
 printf("Введите второе число");
scanf("%f", &y);
 printf("Введите третье число");
scanf("%f", &z);
if ((x==y)||(x==z)||(y==z)) {
    float X,Y,Z;
    X=x+5;
    Y=y+5;
    Z=z+5;
    printf("x=%f\ny=%f\nz=%f",X,Y,Z);
} else {
    printf("Равных нет");
}
return 0;
}
